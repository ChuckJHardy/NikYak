<h1>Listing niks</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th>Votes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @niks.each do |nik| %>
      <tr>
        <td><%= nik.title %></td>
        <td><%= nik.body %></td>
        <td><%= nik.votes %></td>
        <td><%= link_to 'Show', nik %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nik', new_nik_path %>
